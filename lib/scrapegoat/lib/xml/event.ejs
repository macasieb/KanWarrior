BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//fruux//TaskApp//EN
CALSCALE:GREGORIAN
X-WR-CALNAME:Tasks
X-APPLE-CALENDAR-COLOR:#B90E28
BEGIN:VTODO
UID:<%= task.uuid %>
DTSTAMP:<%= task.entry %>
STATUS:<%= task.status %>
PRIORITY:<%= task.priority %>
SUMMARY:<%= task.description %>
<%if (task.due != null) { %>
DUE:<%= task.due %>
<% } %>
END:VTODO
END:VCALENDAR

